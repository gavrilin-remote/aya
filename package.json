{
  "name": "aya-test-task",
  "version": "1.0.0",
  "description": "Test task",
  "private": true,
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "prettier": "npx prettier --ignore-path .eslintignore --check .",
    "prettier:fix": "npx prettier --ignore-path .eslintignore --check --write .",
    "format": "npm run prettier:fix && npm run lint:fix",
    "prepare": "husky install"
  },
  "author": "Oleg Gavrilin",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.10",
    "typeorm-extension": "^2.3.1",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@types/config": "^3.3.2",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.8",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{ts,md}": [
      "prettier --ignore-path .eslintignore --check --write",
      "eslint --fix"
    ]
  }
}
